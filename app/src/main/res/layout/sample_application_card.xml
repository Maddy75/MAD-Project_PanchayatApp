<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:background="#A5E6CE"
    android:padding="5dp">

    <TextView
        android:id="@+id/etRequestNumber"
        android:layout_width="92dp"
        android:layout_height="22dp"
        android:layout_marginTop="4dp"
        android:text="Request Number"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/etAreaName"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:hint="Area"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/etRequestNumber" />

    <TextView
        android:id="@+id/etName"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        android:height="25dp"
        android:hint="Name"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/etRequestNumber" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:text="Area"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/etName"
        app:layout_constraintTop_toBottomOf="@+id/etRequestNumber" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:hint="Name"
        android:textColor="#000000"
        android:textColorHint="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etRequestNumber" />

    <TextView
        android:id="@+id/etPhoneNo"
        android:layout_width="100dp"
        android:layout_height="25dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="24dp"
        android:hint="Phno"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/etName" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:text="Phone"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="50dp"
        android:layout_height="25dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="24dp"
        android:text="Service"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/etPhoneNo"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/etServiceType"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="24dp"
        android:hint="Type"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/textView24"
        app:layout_constraintTop_toBottomOf="@+id/etAreaName" />
</androidx.constraintlayout.widget.ConstraintLayout>